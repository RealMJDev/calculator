<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link rel="stylesheet" href="/css/bundle.css">
    <title>Calculator</title>
</head>
<body class="body">
    
    <div class="calc-container">


        <div class="calc-container-screen">
            <div class="calc-container-screen-sum"></div>
            <div class="calc-container-screen-result">0</div>
        </div>



        <div class="calc-container-button">
            {{!--  onClick="window.run(this);" --}}
            <button class="btn span-col-2 clear">C</button>
            <button class="btn del">DEL</button>
            <button class="btn funk multiply math">*</button>
            <button class="btn math">7</button>
            <button class="btn math">8</button>
            <button class="btn math">9</button>
            <button class="btn funk divide math">/</button>
            <button class="btn math">4</button>
            <button class="btn math">5</button>
            <button class="btn math">6</button>
            <button class="btn funk add math">+</button>
            <button class="btn math">1</button>
            <button class="btn math">2</button>
            <button class="btn math">3</button>
            <button class="btn funk subtract math">-</button>
            <button class="btn point math">.</button>
            <button class="btn math">0</button>
            <button class="btn span-col-2 equal">=</button>
        </div>


    </div>

    <script src="/js/bundle.js"></script>
</body>
</html>